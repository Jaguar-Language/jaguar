class Animal ->
  speak: message ->
    print("Hello! " + message)

class Person::Animal ->
  static count: ->
    print("People Count")
  init: name ->
    @name = name
  say: message ->
    print(message)
  speak: ->
    super("I am a human! My name is " + @name)

jeff = new Person("Jeff")
bob = new Person("Bob")

jeff.say("test")
jeff.speak
Person.count
