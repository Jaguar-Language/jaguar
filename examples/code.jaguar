class Animal ->
  speak: message ->
    print("Hello! " + message)

class Person::Animal ->
  static count: ->
    print("People Count")
  init: name ->
    name = name
  say: message ->
    print(message)
  speak: ->
    super("I am a human!")
    print("Overriding parent")

log: message ->
  print(message)

helloworld ->
  print("Hello, World!")

jeff = Person.new("Jeff")
bob = Person.new("Bob")

jeff.say("test")
Person.say("test")
