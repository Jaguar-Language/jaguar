class Animal ->
  speak: message ->
    print("Hello! " + message)

class Person::Animal ->
  x = 3
  say: message ->
    message
  speak: ->
    super("I am a human!")
    print("Overriding parent")

log: message ->
  print(message)

helloworld ->
  print("Hello, World!")

jeff = Person.new
bob = Animal.new
bob.speak("hello")
jeff.speak
